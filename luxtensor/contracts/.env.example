# ============================================
# LuxTensor Contracts Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!
# ============================================

# Deployer address (use different for each network)
# Local dev - can use Hardhat default: 0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266
DEPLOYER_ADDRESS=

# Private key for signing (NEVER commit real keys!)
# For local dev only - use hardware wallet in production
PRIVATE_KEY=

# RPC endpoints
RPC_URL_LOCAL=http://127.0.0.1:8545
RPC_URL_TESTNET=https://testnet-rpc.luxtensor.network
RPC_URL_MAINNET=https://mainnet-rpc.luxtensor.network

# Token allocation addresses
EMISSION_POOL=
ECOSYSTEM_GRANTS=
DAO_TREASURY=
LIQUIDITY_POOL=
FOUNDATION_RESERVE=

# API Keys (for contract verification, etc.)
ETHERSCAN_API_KEY=
